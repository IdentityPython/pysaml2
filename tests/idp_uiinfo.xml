<?xml version='1.0' encoding='UTF-8'?>
<md:EntitiesDescriptor xmlns:md="urn:oasis:names:tc:SAML:2.0:metadata" xmlns:saml="urn:mace:shibboleth:metadata:1.0" xmlns:saml="urn:oasis:names:tc:SAML:metadata:ui" xmlns:xsi="http://www.w3.org/2000/09/xmldsig#">
    <md:EntityDescriptor entityID="http://example.com/saml2/idp.xml">
        <md:Extensions>
            <saml:Scope regexp="false">descriptor-example.org</saml:Scope>
            <saml:Scope regexp="true">descriptor-example[^0-9]*\.org</saml:Scope>
        </md:Extensions>
        <md:IDPSSODescriptor WantAuthnRequestsSigned="false" protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
            <md:Extensions>
                <saml:Scope regexp="false">idpssodescriptor-example.org</saml:Scope>
                <saml:UIInfo><saml:Keywords xml:lang="en">foo bar</saml:Keywords><saml:Logo height="40" width="30">http://example.com/logo.jpg</saml:Logo><saml:InformationURL>http://example.com/saml2/info.html</saml:InformationURL><saml:DisplayName>Example Co.</saml:DisplayName><saml:Description xml:lang="se">Exempel bolag</saml:Description><saml:PrivacyStatementURL>http://example.com/saml2/privacyStatement.html</saml:PrivacyStatementURL></saml:UIInfo>
            </md:Extensions>
            <md:KeyDescriptor>
                <xsi:KeyInfo><xsi:X509Data><xsi:X509Certificate></xsi:X509Certificate></xsi:X509Data></xsi:KeyInfo>
            </md:KeyDescriptor>
            <md:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect" Location="http://example.com/saml2/" />
        </md:IDPSSODescriptor>
    </md:EntityDescriptor>
</md:EntitiesDescriptor>
